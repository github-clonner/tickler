{
  "name": "Tickler",
  "productName": "Tickler",
  "version": "2.6.1",
  "description": "Media player, transcoder and streamer",
  "keywords": [
    "multimedia",
    "player",
    "encoder",
    "decoder",
    "transcoder",
    "streamer",
    "downloader",
    "playlist"
  ],
  "author": {
    "name": "Benjamin Maggi",
    "email": "benjaminmaggi@gmail.com",
    "url": "https://github.com/maggiben"
  },
  "license": "MIT",
  "main": "main.js",
  "homepage": "https://maggiben.github.io/tickler",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/maggiben/tickler"
  },
  "bugs": {
    "url": "https://github.com/maggiben/tickler/issues"
  },
  "tickler": {
    "settings": {
      "file": "settings.json"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production; electron ./dist",
    "watch": "cross-env TARGET=render webpack --watch",
    "build:clean": "rimraf ./dist",
    "build:main": "cross-env TARGET=main webpack --config webpack.config.babel.js",
    "build:render": "cross-env TARGET=render webpack --config webpack.config.babel.js",
    "build:dev": "cross-env NODE_ENV=development npm run build:main && npm run build:render",
    "build:prod": "cross-env NODE_ENV=production npm run build:main && npm run build:render",
    "pacmac": "build --mac --dir",
    "test": "npm run test:lint && npm run test:typecheck && npm run test:spec",
    "test:lint": "eslint --cache client",
    "test:spec": "cross-env NODE_ENV=test nyc mocha tests/**/*.test.js",
    "test:typecheck": "flow check",
    "coverage": "flow coverage"
  },
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "per-file": true,
    "lines": 20,
    "all": true,
    "exclude": [
      "tests/**",
      "dist/**",
      "flow-typed/**"
    ]
  },
  "build": {
    "productName": "Tickler",
    "appId": "org.develar.Tickler",
    "category": "public.app-category.tools",
    "asar": true,
    "mac": {
      "icon": "./assets/images/icon.icns"
    },
    "directories": {
      "app": "./dist",
      "output": "application"
    },
    "files": [
      "index.html",
      "window.js",
      "bundle.js",
      "vendor.bundle.js",
      "package.json"
    ]
  },
  "dependencies": {
    "@maggiben/duration-format": "0.0.1",
    "@maggiben/electron-clipboard-manager": "git+https://github.com/maggiben/electron-clipboard-manager.git",
    "@maggiben/google-apis": "git+https://github.com/maggiben/google-apis.git",
    "ajv": "^6.2.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.35.0",
    "electron-window-state": "^4.1.1",
    "ffbinaries": "^1.0.7",
    "fluent-ffmpeg": "^2.1.2",
    "flux-standard-action": "^2.0.1",
    "fpcalc": "^1.2.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "howler": "^2.0.9",
    "immutable": "^3.8.2",
    "js-beautify": "^1.7.5",
    "jsonata": "^1.5.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-sortable-hoc": "^0.6.8",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sanitize-filename": "^1.6.1",
    "skeemas": "^1.2.5",
    "urijs": "^1.19.1",
    "uuid": "^3.2.1",
    "wavesurfer.js": "^2.0.4",
    "ytdl-core": "^0.18.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chance": "^1.0.13",
    "copy-webpack-plugin": "^4.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "devtron": "^1.4.0",
    "electron": "^1.8.2",
    "electron-builder": "^20.2.0",
    "electron-connect": "^0.6.3",
    "electron-connect-webpack-plugin": "^0.1.1",
    "electron-packager": "^11.0.1",
    "electron-webpack-plugin": "^2.0.0",
    "eslint": "^4.18.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.66.0",
    "html-webpack-plugin": "^3.0.4",
    "html-webpack-template": "^6.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-simple-vars": "^4.1.0",
    "postcss-url": "^7.3.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.0.0",
    "react-virtualized": "^9.18.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "text-loader": "0.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.0.10",
    "webpack-notifier": "^1.5.1"
  }
}
