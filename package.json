{
  "name": "Tickler",
  "productName": "Tickler",
  "version": "1.0.0",
  "description": "Audio player",
  "author": {
    "name": "Benjamin Maggi",
    "email": "benjaminmaggi@gmail.com",
    "url": "https://github.com/maggiben"
  },
  "license": "MIT",
  "main": "main.js",
  "repository": "git@github.com:maggiben/tickler.git",
  "scripts": {
    "start": "NODE_ENV='production'; electron ./dist",
    "build-main": "webpack --config webpack.base.config.babel.js",
    "build-render": "webpack --config webpack.config.babel.js",
    "package-mac": "electron-packager ./dist/ --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "watch": "webpack --watch",
    "flow": "flow",
    "build-all": "NODE_ENV=production npm run build-main && npm run build-render && npm run start",
    "build-all-dev": "NODE_ENV=development npm run build-main && NODE_ENV=development npm run build-render && npm run start",
    "webpack": "NODE_ENV=production webpack -p",
    "lint": "eslint --cache ./client",
    "lint-fix": "npm run lint -- --fix",
    "pacmac": "build --mac --dir"
  },
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "category": "public.app-category.tools",
    "asar": true,
    "mac": {
      "icon": "./assets/images/icon.icns"
    },
    "directories": {
      "app": "./dist",
      "output": "application"
    },
    "files": [
      "index.html",
      "window.js",
      "bundle.js",
      "vendor.bundle.js",
      "package.json"
    ]
  },
  "dependencies": {
    "@maggiben/duration-format": "0.0.1",
    "async": "^2.5.0",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "electron-window-state": "^4.1.1",
    "history": "^4.6.3",
    "howler": "^2.0.4",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "musicmetadata": "^2.0.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sanitize-filename": "^1.6.1",
    "wavesurfer.js": "^1.4.0",
    "ytdl-core": "^0.14.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chance": "^1.0.10",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.16.0",
    "electron-connect": "^0.6.1",
    "electron-connect-webpack-plugin": "^0.1.1",
    "electron-packager": "^8.7.2",
    "electron-webpack-plugin": "^2.0.0",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "html-webpack-plugin": "^2.29.0",
    "html-webpack-template": "^5.6.0",
    "install": "^0.10.1",
    "json-loader": "^0.5.4",
    "npm": "^5.2.0",
    "nyc": "^11.0.3",
    "postcss": "^6.0.6",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "postcss-url": "^7.0.0",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-cleanup-plugin": "^0.5.1"
  }
}
