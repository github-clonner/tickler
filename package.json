{
  "name": "Tickler",
  "productName": "Tickler",
  "version": "1.0.0",
  "description": "Audio player",
  "author": {
    "name": "Benjamin Maggi",
    "email": "benjaminmaggi@gmail.com",
    "url": "https://github.com/maggiben"
  },
  "license": "MIT",
  "main": "main.js",
  "repository": "git@github.com:maggiben/tickler.git",
  "scripts": {
    "start": "NODE_ENV='production'; electron ./dist",
    "build-main": "webpack --config webpack.base.config.babel.js",
    "build-render": "webpack --config webpack.config.babel.js",
    "package-mac": "electron-packager ./dist/ --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "watch": "webpack --watch",
    "flow": "flow",
    "build-all": "NODE_ENV=production npm run build-main && npm run build-render && npm run start",
    "build-all-dev": "NODE_ENV=development npm run build-main && NODE_ENV=development npm run build-render && npm run start",
    "webpack": "NODE_ENV=production webpack -p",
    "lint": "eslint --cache ./client",
    "lint-fix": "npm run lint -- --fix",
    "pacmac": "build --mac --dir"
  },
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "category": "public.app-category.tools",
    "asar": true,
    "mac": {
      "icon": "./assets/images/icon.icns"
    },
    "directories": {
      "app": "./dist",
      "output": "application"
    },
    "files": [
      "index.html",
      "window.js",
      "bundle.js",
      "vendor.bundle.js",
      "package.json"
    ]
  },
  "dependencies": {
    "@maggiben/duration-format": "0.0.1",
    "async": "^2.1.4",
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "electron-window-state": "^4.0.1",
    "howler": "^2.0.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "musicmetadata": "^2.0.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sanitize-filename": "^1.6.1",
    "wavesurfer.js": "^1.2.8",
    "ytdl-core": "^0.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chance": "^1.0.4",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.6.1",
    "electron-builder": "^13.11.1",
    "electron-connect": "^0.6.1",
    "electron-connect-webpack-plugin": "^0.1.1",
    "electron-packager": "^8.5.2",
    "electron-webpack-plugin": "^1.2.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.39.0",
    "html-webpack-plugin": "^2.24.1",
    "html-webpack-template": "^6.0.0",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "~2.2.1",
    "webpack-cleanup-plugin": "^0.4.1"
  }
}
