{
  "name": "Tickler",
  "productName": "Tickler",
  "version": "1.0.0",
  "description": "Audio player",
  "author": {
    "name": "Benjamin Maggi",
    "email": "benjaminmaggi@gmail.com",
    "url": "https://github.com/maggiben"
  },
  "license": "MIT",
  "main": "main.js",
  "repository": "git@github.com:maggiben/tickler.git",
  "tickler": {
    "settings": {
      "file": "settings.json"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production; electron ./dist",
    "watch": "cross-env TARGET=render webpack --watch",
    "build:clean": "rimraf ./dist",
    "build:mainXX": "webpack --config webpack.base.config.babel.js",
    "build:renderXXX": "webpack --config webpack.config.babel.js",
    "build:main": "cross-env TARGET=main webpack --config webpack.config.babel.js",
    "build:render": "cross-env TARGET=render webpack --config webpack.config.babel.js",
    "build:dev": "cross-env NODE_ENV=development npm run build:main && npm run build:render",
    "build:prod": "NODE_ENV=production npm run build-main && npm run build-render && npm run start",
    "pacmac": "build --mac --dir",
    "test": "npm run test:lint && npm run test:typecheck && npm run test:spec",
    "test:lint": "eslint --cache client",
    "test:spec": "cross-env NODE_ENV=test nyc mocha tests/**/*.test.js",
    "test:typecheck": "flow check",
    "coverage": "flow coverage"
  },
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "per-file": true,
    "lines": 20,
    "all": true,
    "exclude": [
      "tests/**",
      "dist/**",
      "flow-typed/**"
    ]
  },
  "build": {
    "productName": "Tickler",
    "appId": "org.develar.Tickler",
    "category": "public.app-category.tools",
    "asar": true,
    "mac": {
      "icon": "./assets/images/icon.icns"
    },
    "directories": {
      "app": "./dist",
      "output": "application"
    },
    "files": [
      "index.html",
      "window.js",
      "bundle.js",
      "vendor.bundle.js",
      "package.json"
    ]
  },
  "dependencies": {
    "@maggiben/duration-format": "0.0.1",
    "@maggiben/google-apis": "file:../npm_links/google-apis",
    "ajv": "^5.2.3",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "codemirror": "^5.30.0",
    "electron-window-state": "^4.1.1",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "js-beautify": "^1.7.3",
    "jsonata": "^1.3.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tap-event-plugin": "^3.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sanitize-filename": "^1.6.1",
    "skeemas": "^1.2.4",
    "urijs": "^1.18.10",
    "uuid": "^3.1.0",
    "wavesurfer.js": "^1.4.0",
    "ytdl-core": "^0.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "chance": "^1.0.10",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "electron": "^1.7.8",
    "electron-builder": "^19.31.1",
    "electron-connect": "^0.6.1",
    "electron-connect-webpack-plugin": "^0.1.1",
    "electron-packager": "^9.1.0",
    "electron-webpack-plugin": "^2.0.0",
    "eslint": "^4.7.2",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.56.0",
    "html-webpack-plugin": "^2.29.0",
    "html-webpack-template": "^5.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "postcss": "^6.0.12",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.1.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "text-loader": "0.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-notifier": "^1.5.0"
  }
}
