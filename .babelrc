{
  "presets": [
    ["env", {
      "targets": {
        "electron": "1.7"
      },
      "useBuiltIns": true
    }],
    "react",
    "stage-0"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  },
  "sourceMaps": true,
  "compact": false,
  "plugins": [
    "transform-decorators-legacy",
    "transform-async-to-generator",
    "transform-class-properties",
    "transform-flow-strip-types"
  ]
}
